<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Athelia" />
		<Option makefile="SConstruct" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/debug/block" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/debug" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<ExtraCommands>
					<Add before='cmd /c &quot;cp src\libathelia.a lib\win32\debug&quot;' />
					<Add after="mv ./techdemos/block/block.exe ./bin/debug" />
				</ExtraCommands>
				<MakeCommands>
					<Build command="cmd /c scons -f $makefile debug=1" />
					<CompileFile command="cmd /c scons -f $makefile debug=1 $file" />
					<Clean command="cmd /c scons -f $makefile debug=1 -c" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="cmd /c scons -f $makefile debug=1 -q" />
					<SilentBuild command="cmd /c scons -f $makefile debug=1 &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/release/block" prefix_auto="1" extension_auto="1" />
				<Option working_dir="bin/release" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<ExtraCommands>
					<Add before='cmd /c &quot;cp src\libathelia.a lib\win32\release&quot;' />
					<Add after="mv ./techdemos/block/block.exe ./bin/release" />
				</ExtraCommands>
				<MakeCommands>
					<Build command="cmd /c scons -f $makefile" />
					<CompileFile command="cmd /c scons -f $makefile $file" />
					<Clean command="cmd /c scons -f $makefile -c" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="cmd /c scons -f $makefile -q" />
					<SilentBuild command="cmd /c scons -f $makefile &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="SConstruct" />
		<Unit filename="src/SConscript" />
		<Unit filename="src/backend/block.cpp" />
		<Unit filename="src/backend/block.h">
			<Option compile="1" />
		</Unit>
		<Unit filename="src/backend/controller.h">
			<Option compile="1" />
		</Unit>
		<Unit filename="src/backend/cube.cpp" />
		<Unit filename="src/backend/cube.h">
			<Option compile="1" />
		</Unit>
		<Unit filename="src/backend/gamesystem.cpp" />
		<Unit filename="src/backend/gamesystem.h">
			<Option compile="1" />
		</Unit>
		<Unit filename="src/backend/graphics.cpp" />
		<Unit filename="src/backend/graphics.h">
			<Option compile="1" />
		</Unit>
		<Unit filename="src/backend/input.cpp" />
		<Unit filename="src/backend/input.h">
			<Option compile="1" />
			<Option link="1" />
		</Unit>
		<Unit filename="src/backend/patch.h">
			<Option compile="1" />
		</Unit>
		<Unit filename="techdemos/SConscript" />
		<Unit filename="techdemos/block/main.cpp" />
		<Unit filename="techdemos/template/main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
			<DoxyBlocks>
				<comment_style block="0" line="0" />
